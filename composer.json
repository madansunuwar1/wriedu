{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "anuzpandey/laravel-nepali-date": "v3.0.0",
        "google/apiclient": "v2.18.3",
        "laravel/framework": "v11.31.0",
        "laravel/sanctum": "v4.2.0",
        "laravel/tinker": "v2.10.1",
        "league/commonmark": "2.7.1",
        "phpoffice/phpspreadsheet": "3.11.0",
        "predis/predis": "v2.4.0",
        "pusher/pusher-php-server": "7.2.7",
        "spatie/laravel-activitylog": "4.10.2",
        "spatie/laravel-permission": "6.21.0"
    },
    "require-dev": {
        "fakerphp/faker": "v1.24.1",
        "laravel/breeze": "v2.3.8",
        "laravel/pail": "v1.2.3",
        "laravel/pint": "v1.24.0",
        "laravel/sail": "v1.44.0",
        "mockery/mockery": "1.6.12",
        "nunomaduro/collision": "v8.5.0",
        "phpunit/phpunit": "11.5.31"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}